<?xml version="1.0" ?>
<openerp>
    <data>
        <record id="indaws_sending_company" model="ir.ui.view">
            <field name="name">res.company.sending</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Mensajeria Sending">                      
                        <group>
                            <field name="sending_username"/>
                            <field name="sending_password" />                              
                            <field name="sending_center"/>
                        </group>
                    </page>
                </notebook>
            </field>
       </record>    
    </data>
</openerp>
